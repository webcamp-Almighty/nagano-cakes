<div class="row">
  <div class="col-md-3 col-md-offset-2">
    <h3>会員一覧</h3>
  </div>
</div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th colspan="2">ステータス</th>
        </tr>
      </thead>

      <tbody>
      <% @end_users.each do |end_user| %>
        <tr>
          <td><%= end_user.id %></td>
          <td><%= "#{end_user.last_name}#{end_user.first_name}" %></td>
          <td><%= end_user.email %></td>
          <td><%if end_user.is_deleted == true %>
                  退会
              <% else %>
                  有効
              <% end %>
          </td>
          <td><%= link_to "詳細ページ", admins_end_user_path(end_user.id), class: "btn btn-primary" %></td>
        </tr>
      <% end %>
      <tbody>
      </table>
    <div id="pagination">
      <%= paginate @end_users %>
    </div>
  </div>
  </div>
