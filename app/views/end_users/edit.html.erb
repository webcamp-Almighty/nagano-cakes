<h2>登録情報編集</h2>
<table class="edit-end-user table">
	<%= form_for @end_user do |f| %>
	<tr>
		<th>名前</th>
		<td>(姓)  <%= f.text_field :last_name %></td>
		<td>(名)  <%= f.text_field :first_name %></td>
	</tr>
	<tr>
		<th>フリガナ</th>
		<td><%= f.text_field :name_kana %></td>
		<td><%= f.text_field :name_kana %></td>
	</tr>
	<tr>
		<th>メールアドレス</th>
		<td colspan="2"><%= f.text_field :email %></td>
	</tr>
	<tr>
		<th>郵便番号(ハイフンなし)</th>
		<td colspan="2"><%= f.text_field :postal_code %></td>
	</tr>
	<tr>
		<th>住所</th>
		<td colspan="2"><%= f.text_field :address %></td>
	</tr>
	<tr>
		<th>電話番号</th>
		<td colspan="2"><%=f.text_field :telephone_number %></td>
	</tr>
    <tr>
		<th></th>
		<td><%= f.submit '編集内容を保存する', class: "btn btn-primary" %></td>
		<td><%= link_to '退会する', end_users_confirm_path, class: "btn btn-danger" %></td>
	</tr>
	<%= end %>
</table>
